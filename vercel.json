{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/mi-proyecto01/browser",
  "framework": "none",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
